{
	"name": "wiki-quick-edit",
	"version": "0.0.0",
	"description": "A MediaWiki gadget used to allow users to quickly edit and contribute.",
	"type": "module",
	"scripts": {
		"build": "pnpm format && pnpm build:only",
		"build:only": "tsx --no-deprecation scripts/build.ts",
		"server": "tsx --no-deprecation scripts/server.ts",
		"format": "prettier --cache --write . && pnpm lint:fix && tsc",
		"eslint": "eslint --cache .",
		"eslint:fix": "eslint --cache --fix .",
		"stylelint": "stylelint --allow-empty-input --cache src/**/*.{css,less} ",
		"stylelint:fix": "stylelint --allow-empty-input --cache --fix src/**/*.{css,less} ",
		"lint": "pnpm eslint && pnpm stylelint",
		"lint:fix": "pnpm eslint:fix && pnpm stylelint:fix",
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky"
	},
	"author": {
		"name": "AnYiEE",
		"email": "i@anyi.in"
	},
	"contributors": [
		"Other contributors <https://github.com/AnYiEE/WikiQuickEdit/graphs/contributors>"
	],
	"license": "GPL-3.0-or-later",
	"homepage": "https://github.com/AnYiEE/WikiQuickEdit",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AnYiEE/WikiQuickEdit.git"
	},
	"bugs": {
		"url": "https://github.com/AnYiEE/WikiQuickEdit/issues"
	},
	"keywords": [
		"Gadget",
		"MediaWiki",
		"MediaWiki Gadget",
		"WikiQuickEdit"
	],
	"devDependencies": {
		"@babel/core": "^7.24.0",
		"@babel/helper-compilation-targets": "^7.23.6",
		"@babel/helper-plugin-utils": "^7.24.0",
